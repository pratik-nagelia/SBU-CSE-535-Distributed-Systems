import json
import sys
import time
from run import Run


def main():
    config(clock='Lamport', channel='fifo')
    with open(sys.argv[1]) as config_file:
        configs = json.load(config_file)
        
    for conf in configs:
        run = new(Run, num=1)
        setup(run, (conf,))
        start(run)
        time.sleep(10)


if __name__ == '__main__':
    main()
