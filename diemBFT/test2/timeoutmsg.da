from tc import TC
from qc import QC
from timeoutinfo import TimeoutInfo


class TimeoutMessage:
    def setup(timeout_info:TimeoutInfo, last_round_tc:TC, high_commit_qc:QC, signature:str):
        self.timeout_info = timeout_info
        self.last_round_tc = last_round_tc
        self.high_commit_qc = high_commit_qc
